local function randomString(l)
local m,n,
o,
p=string.sub,math.random,"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ",""
for q=1,l do
local s=n(1,#o)
p=p..m(o,s,s)
end
return p
end














return false,(randomString(256)..((math.random()>=0.5)and"=="or"="))
end

