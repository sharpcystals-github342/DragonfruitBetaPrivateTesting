end)()end

local function randomString(aa)
local ab,
ac="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ",""
for ad=1,aa do
local ae=math.random(1,#ab)
ac=ac..string.sub(ab,ae,ae)
end
return ac
end

