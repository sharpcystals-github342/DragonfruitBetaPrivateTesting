local function randomString(m)
local n,
o="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ",""
for p=1,m do
local q=math.random(1,#n)
o=o..string.sub(n,q,q)
end
return o
end

