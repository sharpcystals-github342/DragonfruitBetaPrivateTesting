local aK,
aL,
aM={},tonumber(aI[1]),{table.unpack(aJ)}
for aN=1,aL do
if#aM==0 then break end
local aO=math.random(1,#aM)
table.insert(aK,aM[aO])
table.remove(aM,aO)
end
return aK
